arm_64bit=1
<% if $facts['profile']['platform'] == 'raspberrypi4' { -%>
arm_freq=2000
over_voltage=6
<% } -%>
disable_overscan=1
disable_splash=1
<% if $facts['profile']['platform'] == 'raspberrypi4' { -%>
dtparam=act_led_trigger=actpwr
dtoverlay=vc4-kms-v3d
uart_2ndstage=1
<% } -%>
enable_uart=1
kernel=u-boot.bin
